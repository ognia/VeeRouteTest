    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.3.0",
    "html-webpack-plugin": "^2.7.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "node-libs-browser": "2.0.0",
    "node-sass": "^4.5.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "1.2.2",
    "raw-loader": "^0.5.1",
    "resolve-url": "^0.2.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.0",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
